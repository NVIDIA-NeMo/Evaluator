{
    "categories": [
      {
        "title": "## Evaluation\n\n",
        "labels": ["evaluation", "eval", "metrics", "benchmark"],
        "exclude_labels": ["ignore"]
      },
      {
        "title": "\n## LLM Evaluation\n\n",
        "labels": ["llm", "language-model", "generation"],
        "exclude_labels": ["ignore"]
      },
      {
        "title": "\n## Multimodal Evaluation\n\n",
        "labels": ["multimodal", "mm", "vision", "vlm"],
        "exclude_labels": ["ignore"]
      },
      {
        "title": "\n## Datasets & Tasks\n\n",
        "labels": ["dataset", "task", "data"],
        "exclude_labels": ["ignore"]
      },
      {
        "title": "\n## Infrastructure\n\n",
        "labels": ["infrastructure", "deployment", "server"],
        "exclude_labels": ["ignore"]
      },
      {
        "title": "\n## Documentation\n\n",
        "labels": ["docs", "documentation"],
        "exclude_labels": ["ignore"]
      },
      {
        "title": "\n## CI/CD\n\n",
        "labels": ["ci", "github-actions", "workflow", "testing"],
        "exclude_labels": ["ignore"]
      },
      {
        "title": "\n## Bug Fixes\n\n",
        "labels": ["bug", "bugfix", "fix"],
        "exclude_labels": ["ignore"]
      }
    ],
    "ignore_labels": [
      "ignore",
      "skip-changelog"
    ],
    "sort": "ASC",
    "template": "\n${{CHANGELOG}}\n## Others\n\n${{UNCATEGORIZED}}\n",
    "pr_template": "- ${{TITLE}} [#${{NUMBER}}](${{URL}})",
    "empty_template": "- No changes in this release",
    "label_extractor": [
      {
        "pattern": "(.*evaluat.*)|(.*\\beval\\b.*)|(.*metric.*)|(.*benchmark.*)|(.*score.*)",
        "target": "evaluation",
        "flags": "gimu",
        "on_property": ["title", "body"]
      },
      {
        "pattern": "(.*\\bllm\\b.*)|(.*language.*model.*)|(.*generation.*)|(.*text.*)|(.*gpt.*)",
        "target": "llm",
        "flags": "gimu",
        "on_property": ["title", "body"]
      },
      {
        "pattern": "(.*multimodal.*)|(.*\\bmm\\b.*)|(.*vision.*)|(.*image.*)|(.*vlm.*)|(.*video.*)",
        "target": "multimodal",
        "flags": "gimu",
        "on_property": ["title", "body"]
      },
      {
        "pattern": "(.*dataset.*)|(.*task.*)|(.*\\bdata\\b.*)|(.*loader.*)",
        "target": "dataset",
        "flags": "gimu",
        "on_property": ["title", "body"]
      },
      {
        "pattern": "(.*infrastructure.*)|(.*deploy.*)|(.*server.*)|(.*api.*)",
        "target": "infrastructure",
        "flags": "gimu",
        "on_property": ["title", "body"]
      },
      {
        "pattern": "(.*doc.*)|(.*readme.*)|(.*guide.*)|(.*tutorial.*)",
        "target": "docs",
        "flags": "gimu",
        "on_property": ["title", "body"]
      },
      {
        "pattern": "(.*\\bci\\b.*)|(.*github.*)|(.*workflow.*)|(.*action.*)|(.*test.*)",
        "target": "ci",
        "flags": "gimu",
        "on_property": ["title", "body"]
      },
      {
        "pattern": "(.*\\[bug.*)|(.*\\bfix\\b.*)|(.*bugfix.*)|(.*patch.*)",
        "target": "bug",
        "flags": "gimu",
        "on_property": ["title", "body"]
      }
    ],
    "duplicate_filter": {
      "pattern": ".+",
      "on_property": "title",
      "method": "match"
    },
    "transformers": [
    ],
    "max_tags_to_fetch": 100,
    "max_pull_requests": 500,
    "max_back_track_time_days": 365,
    "exclude_merge_branches": [
    ],
    "tag_resolver": {
      "method": "semver"
    }
}

